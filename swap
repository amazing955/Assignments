#include <stdio.h>
#include <stdlib.h>

void swap(int x,int y){
int temp = x;
x = y;
y = temp;
}
int main()
{
    int a;
    printf("enter a number\n");
    scanf("%d",&a);
    int b;
    printf("enter a number\n");
    scanf("%d",&b);
    printf("before swap:\n a = %d\n b = %d\n",a,b);
    swap(a, b);
    printf("after swap:\n a = %d\n b = %d",b,a);
        return 0;

    }
