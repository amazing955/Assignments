#include <stdio.h>

int main() {
    char z[4][10];  
    char p[4][10];

    printf("Enter the domestic animals:\n");
    for(int x = 0; x < 4; x++) {
        scanf("%s",&z[x]);  
    }

    printf("Enter the wild animals:\n");
    for(int s = 0; s < 4; s++) {
        scanf("%s",&p[s]);  
    }

    printf("list of all animals:\n");
    for(int i = 0; i < 4; i++) {
        printf("%s\t", z[i]); 
printf("\t");
        printf("%s\n", p[i]);  
    }
printf("...END OF PROGRAM...");
    return 0;
}
